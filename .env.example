# Example environment variables (DO NOT commit real secrets)
# Copy to .env and fill in real values: cp .env.example .env  (or on Windows: copy .env.example .env)

# Supabase (client-side)
VITE_SUPABASE_PROJECT_ID=REPLACE_WITH_PROJECT_ID
VITE_SUPABASE_PUBLISHABLE_KEY=REPLACE_WITH_PUBLISHABLE_KEY
VITE_SUPABASE_URL=https://REPLACE_WITH_PROJECT_ID.supabase.co

# Instagram / Meta (server-side)
INSTAGRAM_APP_ID=REPLACE_WITH_APP_ID
INSTAGRAM_APP_SECRET=REPLACE_WITH_APP_SECRET
INSTAGRAM_VERIFY_TOKEN=REPLACE_WITH_VERIFY_TOKEN
INSTAGRAM_ACCOUNT_ID=REPLACE_WITH_ACCOUNT_ID
INSTAGRAM_ACCESS_TOKEN=REPLACE_WITH_ACCESS_TOKEN

# Supabase service role key (server-side only)
SUPABASE_SERVICE_ROLE_KEY=REPLACE_WITH_SERVICE_ROLE_KEY

# Optional frontend vars (exposed to browser â€” prefix with VITE_):
VITE_INSTAGRAM_APP_ID=REPLACE_WITH_APP_ID
VITE_INSTAGRAM_REDIRECT_URI=http://localhost:5173/auth/instagram/callback

# Google OAuth (local dev)
GOOGLE_CLIENT_ID=REPLACE_WITH_CLIENT_ID
GOOGLE_CLIENT_SECRET=REPLACE_WITH_CLIENT_SECRET
SESSION_SECRET=REPLACE_WITH_SESSION_SECRET
CLIENT_URL=http://localhost:8080
VITE_GOOGLE_CLIENT_ID=REPLACE_WITH_CLIENT_ID
